{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "apigee-issuer",
    "Audience": "audience-sample",
    "ValidateAudience": true,
    "ValidateIssuer": true,
    "RequireHttpsMetadata": false,
    "IncludeErrorDetails": true,
    "ClockSkew": 0,
    "PublicKeyValue": {
      "Modulus": "",
      "Exponent": "AQAB"
    }
  },
  "JwtIssuer": {
    "Issuer": "apigee-issuer",
    "Audience": "audience-sample",
    "ExpiresMinutes": 60,
    "PrivateKeyValue": {
      "Modulus": "",
      "Exponent": "AQAB",
      "P": "",
      "Q": "",
      "DP": "",
      "DQ": "",
      "InverseQ": "",
      "D": ""
    }
  },

  "FusapAuthorizationClient": {
    "Authorities": [
      {
        "ConnectionString": "http://localhost:8083/grant",
        "Resources": [
          "urn:fusap:"
        ]
      }
    ]
  }
}
